{
  "name": "initio",
  "version": "0.9.8",
  "private": true,
  "description": "Bestandstool",
  "author": "Neupi",
  "homepage": "./",
  "proxy": "http://localhost:5000",
  "main": "public/electron.js",
  "dependencies": {
    "@hapi/joi": "15.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.6.0",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-tabs": "^3.2.2",
    "web-vitals": "^1.0.1"
  },
  "build": {
    "appId": "dev.neubauer.initio",
    "productName": "Initio",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "extraResources": [
      {
        "from": "../Backend/db.sqlite",
        "to": "db/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "icon": "build/icon.ico",
      "target": [
        "7z"
      ]
    },
    "nsis": {
      "installerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "deleteAppDataOnUninstall": true
    }
  },
  "scripts": {
    "forge": "electron-forge start",
    "build-react": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "client": "yarn start",
    "server": "cd ../Backend && nodemon index.js",
    "dev": "concurrently \"yarn run server\" \"yarn run client\" ",
    "electron": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "electron-builder install-app-deps",
    "pack-app": "yarn build && electron-builder --dir",
    "pack-build": "electron-builder --dir",
    "build": "yarn build-react && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^12.0.4",
    "electron-builder": "^23.6.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "initio"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
